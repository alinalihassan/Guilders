{
  "name": "@guilders/api",
  "version": "1.0.0",
  "module": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:secrets": "wrangler secret bulk .env.production",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "cf-typegen": "wrangler types",
    "ngrok": "bun --env-file=.env __ngrok-command",
    "auth:generate": "bunx --bun auth generate --output src/db/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:init": "bun --env-file=.env scripts/init-db.ts",
    "__ngrok-command": "ngrok http 3000 --authtoken=$NGROK_AUTHTOKEN --url=${NGROK_URL}"
  },
  "dependencies": {
    "@better-auth/api-key": "catalog:",
    "@better-auth/drizzle-adapter": "catalog:",
    "@better-auth/expo": "catalog:",
    "@better-auth/oauth-provider": "catalog:",
    "@better-auth/passkey": "catalog:",
    "@better-auth/stripe": "catalog:",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/openapi": "^1.4.14",
    "@guilders/transactional": "workspace:*",
    "@json-render/core": "^0.11.0",
    "@modelcontextprotocol/sdk": "1.27.1",
    "@neondatabase/serverless": "^1.0.2",
    "@sinclair/typebox": "^0.34.48",
    "agents": "^0.6.0",
    "ai": "catalog:",
    "ai-gateway-provider": "^3.1.1",
    "better-auth": "catalog:",
    "drizzle-orm": "1.0.0-beta.15-859cf75",
    "drizzle-typebox": "^0.3.3",
    "elysia": "1.4.27",
    "fast-json-stable-stringify": "^2.1.0",
    "jose": "^6.1.3",
    "resend": "^6.9.3",
    "snaptrade-typescript-sdk": "^9.0.168",
    "stripe": "^20.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "auth": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "drizzle-kit": "1.0.0-beta.15-859cf75",
    "wrangler": "catalog:"
  }
}
