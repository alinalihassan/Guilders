{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "guilders-docs",
  "compatibility_date": "2026-02-27",
  "assets": {
    "directory": "./out",
    "not_found_handling": "404-page",
  },
  "route": {
    "custom_domain": true,
    "pattern": "docs.guilders.app",
  },
  "workers_dev": false,
  "preview_urls": false,
  "observability": {
    "enabled": true,
    "traces": {
      "enabled": true,
    },
  },
}
